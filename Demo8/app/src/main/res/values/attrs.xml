<?xml version="1.0" encoding="utf-8"?>
<resources>



    <declare-styleable name="TriangleView">
        <!--模式-->
        <attr name="tlv_mode">
            <!--倒三角-->
            <enum name="inverted" value="0" />
            <!--正三角-->
            <enum name="regular" value="1" />
        </attr>
        <!--填充颜色-->
        <attr name="tlv_fill_color" format="color|reference" />
        <!--描边颜色-->
        <attr name="tlv_stroke_color" format="color|reference" />
    </declare-styleable>



</resources>